}
material banger_fdoor_t2 : cd/Reflective
{
    set $specular_map banger_fdoor_t2_s.jpg
    set $diffuse_map  banger_fdoor_t2_d.jpg
}


material banger_rdoor_uv : cd/Reflective
{
    set $specular_map banger_rdoor_uv_s.jpg
    set $diffuse_map  banger_rdoor_uv_d.jpg
}
material banger_rdoor_t1 : cd/Reflective
